   "version": "0.2.0",
   "configurations": [
      {
         "type": "node",
         "request": "launch",
         "name": "yo team",
         "program": "${workspaceRoot}/node_modules/yo/lib/cli.js",
         "args": ["team"],
         "stopOnEntry": true,
         "cwd": "${workspaceRoot}",
         "console": "integratedTerminal",
         "internalConsoleOptions": "neverOpen"        
